{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "src/**/*", "tests/**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@services": ["./src/services/"]
